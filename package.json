{
  "name": "trading-vue3-js",
  "version": "1.0.2",
  "description": "Hackable charting lib for traders",
  "main": "./dist/trading-vue.js",
  "scripts": {
    "test": "webpack-dev-server --config webpack/test.config.js --mode development --progress --hot",
    "auto-test": "ava",
    "eslint": "./node_modules/.bin/eslint --ext .js,.vue src",
    "dev": "webpack-dev-server --config webpack/dev.config.js --mode development --progress --hot",
    "ww": "webpack-dev-server --config webpack/dev.config.js --mode production --progress --hot",
    "build": "webpack  --config webpack/build.config.js --mode production --progress",
    "cdn": "webpack-dev-server --config webpack/cdn.config.js --mode development --progress --hot"
  },
  "ava": {
    "babel": {
      "compileAsTests": [
        "test/auto_tests/**/*",
        "src/**/*"
      ]
    },
    "files": [
      "test/auto_tests/**/*"
    ]
  },
  "author": "C451",
  "contributors": [
    {
      "name": "sudoshu"
    }
  ],
  "license": "MIT",
  "files": [
    "dist/*",
    "src/*"
  ],
  "keywords": [
    "trading",
    "vue",
    "charts",
    "crypto",
    "visualization",
    "reactive",
    "algo-trading",
    "finviz",
    "trading-systems"
  ],
  "homepage": "https://github.com/sudoshu/trading-vue3-js#readme",
  "repository": "github:sudoshu/trading-vue3-js",
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-parameters": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@vue/compiler-sfc": "^3.2.33",
    "ava": "^4.2.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.14.0",
    "eslint-config-eslint": "^7.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "html-webpack-plugin": "^5.5.0",
    "null-loader": "^4.0.1",
    "raw-loader": "^4.0.2",
    "request": "^2.88.2",
    "terser-webpack-plugin": "^5.3.1",
    "tulind": "^0.8.20",
    "vue-codemirror": "^4.0.6",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "arrayslicer": "^1.2.3",
    "hammerjs": "^2.0.8",
    "hamsterjs": "^1.1.3",
    "lz-string": "^1.4.4",
    "vue": "^3.2.33"
  }
}
